begin calculation ML_BSSN_convertFromADMBaseGamma
  dir^a   = sign(beta^a)
  detgt   = 1
  gtu^ab  = detgt**-1 * (-1 * gt13**2 * gt22 + 2 * gt12 * gt13 * gt23 + -1 *
