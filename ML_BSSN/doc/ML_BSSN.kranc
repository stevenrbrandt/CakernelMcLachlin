begin calculation ML_BSSN_convertFromADMBase
  g_ab   = admg_ab
  detg   = -1 * g13**2 * g22 + 2 * g12 * g13 * g23 + -1 * g11 * g23**2 + -1 *
           g12**2 * g33 + g11 * g22 *
           g33
